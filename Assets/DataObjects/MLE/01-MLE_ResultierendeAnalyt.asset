  mleName: "Resultierende analytisch l\xF6sen"
  mleHandout: {fileID: 11400000, guid: 44f2356bc0becdc43b2ad281b8d23d1d, type: 2}
  mleIntroText: "Weniger ist mehr. Hier findest du raus, wie viele einzelne Kr\xE4fte
